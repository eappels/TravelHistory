<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TravelHistoryApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:TravelHistoryApp.Views"
    Title="TravelHistoryApp">

    <ShellContent
        Title="Map"
        ContentTemplate="{DataTemplate views:MapView}"
        Route="MapView" />

    <ShellContent
        Title="History"
        ContentTemplate="{DataTemplate views:HistoryView}"
        Route="HistoryView" />

</Shell>